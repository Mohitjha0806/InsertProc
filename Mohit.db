CREATE DATABASE MOHIT;


CREATE TABLE User_detailsS
(
ID INT IDENTITY(1,1) PRIMARY KEY,
FIRST_NAME VARCHAR(20),
LAST_NAME VARCHAR(20),
EMAIL VARCHAR(20),
MOBILE_NUMBER VARCHAR(13),
USER_NAME VARCHAR(20),
PASSWORD VARCHAR(20),
DATE_OF_BIRTH DATE,
TIME TIME,
AGE VARCHAR(10),
GENDER VARCHAR(20),
EDUCATION VARCHAR(20),
COUNTRY VARCHAR(20),
STATE VARCHAR(20),
CITY VARCHAR(20),
ADDRESS VARCHAR(200),
);



SELECT * FROM User_detailsS;

drop proc insertAll
CREATE PROCEDURE insertAll
(
@FIRST_NAME VARCHAR(20) = NULL,
@LAST_NAME VARCHAR(20) = NULL,
@EMAIL VARCHAR(20) = NULL,
@MOBILE_NUMBER VARCHAR(13) = NULL,
@USER_NAME VARCHAR(20) = NULL,
@PASSWORD VARCHAR(20) = NULL,
@DATE_OF_BIRTH DATE = NULL,
@TIME TIME = NULL,
@AGE VARCHAR = NULL,
@GENDER VARCHAR(20) = NULL,
@EDUCATION VARCHAR(20) = NULL,
@COUNTRY VARCHAR(20) = NULL,
@STATE VARCHAR(20) = NULL,
@CITY VARCHAR(20) = NULL,
@ADDRESS VARCHAR(200) = NULL)

AS
BEGIN

INSERT INTO User_detailsS([FIRST_NAME], [LAST_NAME], [EMAIL], [MOBILE_NUMBER], [USER_NAME], [PASSWORD], [DATE_OF_BIRTH], [TIME], [AGE], [GENDER], [EDUCATION], [COUNTRY], [STATE], [CITY], [ADDRESS])
VALUES(
@FIRST_NAME,
@LAST_NAME,
@EMAIL,
@MOBILE_NUMBER,
@USER_NAME,
@PASSWORD,
@DATE_OF_BIRTH,
@TIME,
@AGE,
@GENDER,
@EDUCATION,
@COUNTRY,
@STATE,
@CITY,
@ADDRESS)
END
